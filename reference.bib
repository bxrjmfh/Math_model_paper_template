
@article{2,
	title = {飞机机动划分的数据挖掘方法},
	volume = {34},
	issn = {1000-2758},
	url = {https://kns.cnki.net/kcms/detail/detail.aspx?dbcode=CJFD&dbname=CJFDLAST2016&filename=XBGD201601006&uniplatform=NZKPT&v=Wj-On_0Ycs0jXKZTWs_Fco6zZnyRYoZEygAYmN_W3uPkmqX4IVCrnerYAIAg0oXC},
	abstract = {飞机机动划分是将飞行数据分解成若干具有明确物理意义的机动动作子序列的重要前处理方法,也是健康监控、飞行模拟、飞行品质评估等研究工作的必要步骤。结合数据挖掘技术提出一种自动的飞机机动划分方法,该方法根据法向过载数据的趋势提取出飞行数据中的机动片段,并用{ISODATA聚类将机动片段归并为若干分类},可以证明每个分类代表一种机动动作。将该方法分别应用于小规模飞行数据与大规模飞行数据中能够识别并正确划分至少89\%的机动动作,证明该方法有效且满足工程精度要求。},
	pages = {33--40},
	number = {1},
	journaltitle = {西北工业大学学报},
	author = {殷, 之平 and 刘, 飞 and 黄, 其青},
	urldate = {2022-06-28},
	date = {2016},
	langid = {pinyin},
	note = {{\textless}北大核心, {EI}, {CSCD}{\textgreater}},
	keywords = {数据挖掘, data mining, time series, 机动划分, 趋势识别, aircraft, algorithms, cluster analysis, data fusion, eigenvalues and eigenfunctions, genetic algorithms, {ISODATA} clustering, {ISODATA聚类}, least squares approximation, linear regression, maneuver partition, non-supervised learning, signal to noise ratio, trend recognition},
	file = {Full Text PDF:files/231/张 等。 - 2016 - 飞机机动划分的数据挖掘方法.pdf:application/pdf},
}

