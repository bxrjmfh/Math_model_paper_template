@article{9,
	title = {新冠疫情对服务行业的影响研究——以南京市为例},
	issn = {1673-5889},
	url = {http://kns-cnki-net-s.vpn.uestc.edu.cn:8118/kcms/detail/detail.aspx?dbcode=CJFD&dbname=CJFDLAST2022&filename=XDBY202134008&uniplatform=NZKPT&v=mxUAR9zyeekPYhcAdngiYX5XdeoIsqH7mqhc8FXoMZTtf3EROuLrRhJDffq1kjL8},
	doi = {10.14097/j.cnki.5392/2021.34.008},
	abstract = {2020年初的新冠疫情给我国的国民经济和社会发展带来了较大的影响,对服务行业带来了一定的负面冲击。本文以南京市服务行业为对象,探讨新冠疫情对服务行业发展趋势的影响。从企业的生产经营、资金运营、销售模式、用工管理四个方面分析企业受到的冲击,分析服务业企业因疫情影响所遇到的困难和问题,并提出对策。},
	pages = {27--30},
	number = {34},
	journaltitle = {现代商业},
	author = {赵, 楷文 and 周, 呈妍 and 钱, 艺萱 and 胡, 雯颖 and 杨, 涛},
	urldate = {2022-08-28},
