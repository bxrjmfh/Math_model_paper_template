
@thesis{__2020,
	title = {A银行中小企业信贷风险管理研究},
	url = {http://kns-cnki-net-s.vpn.uestc.edu.cn:8118/kcms/detail/detail.aspx?filename=1021039372.nh&dbname=CMFDTEMP&dbcode=CMFD&v=Udtm5gjxBsjRf1j2KfxOl3oQjmcQOzQBWwl0UpCjteV0AfbMQHKzG1j6GcJecD4m&uniplatform=NZKPT},
	abstract = {中小企业是国家经济发展的重要力量,对解决就业、活跃市场、实现科技创新具有重要推进作用。随着市场经济的不断完善,中小企业的融资需求日渐增多。在国家政策的支持下,各类型商业银行开始加大对中小企业信贷投放力度。A银行作为国有大型商业银行,近年来大力发展中小企业信贷业务,已经搭建了互联网大数据平台,为中小企业信贷融资提供了极大的便利性。中小企业通过A银行的互联网大数据平台申请贷款,能够实现“秒申、秒审、秒到账”。然而新型冠状病毒感染肺炎疫情已在全球蔓延,众多中小企业的生产经营遭受了沉重的打击,而商业银行中小企业信贷违约风险也随之提高。为了降低商业银行信贷违约风险发生的可能性,减少违约损失,本文选择对商业银行中小企业信贷风险管理情况进行研究。本文以信息不对称理论、“5C”要素分析理论、全面风险管理理论为理论基础,选择国有银行A银行作为研究对象,旨在研究A银行中小企业信贷风险管理现状,分析现阶段中小企业信贷风险管理存在的问题,探寻提升A银行中小企业信贷风险管理水平的对策。研究内容主要分为四个方面:一是简述中小企业概念及划分标准,并介绍中小企业信贷风险的特点;二是介绍A银行概况及中小企业信贷业务的发展现状;三是阐述A银行中小企业信贷风险管理存在的问题;四是提出A银行信贷风险管理的提升措施。当前,受新型冠状病毒肺炎影响,我国宏观经济有下行的趋势。当前经济形势下,对于商业银行而言,有效控制中小企业信贷风险是非常重要的。通过数据研究表明,虽然A银行中小企业风险管理在同业中处于领先地位,但是仍然存在一些问题。希望通过本文的研究,可以为A银行寻找信贷风险管理的提升措施,同时为其他商业银行提供借鉴和参考。},
	institution = {东北财经大学},
	type = {硕士},
	author = {杨, 文韬},
	editora = {许, 浩然},
	editoratype = {collaborator},
	urldate = {2022-08-25},
	date = {2020},
	langid = {pinyin},
	doi = {10.27006/d.cnki.gdbcu.2020.001764},
	keywords = {风险管理, 信贷风险, 银行, 中小企业, Bank, Credit risk, Risk management, Small and medium-sized enterprises},
	file = {杨 - 2020 - A银行中小企业信贷风险管理研究.pdf:D\:\\WEATH\\storage\\8USTIIGA\\杨 - 2020 - A银行中小企业信贷风险管理研究.pdf:application/pdf},
}

@thesis{_sd_2018,
	title = {农商行{SD分行中小企业信贷业务风险评估及其策略研究}},
	url = {http://kns-cnki-net-s.vpn.uestc.edu.cn:8118/kcms/detail/detail.aspx?dbcode=CMFD&dbname=CMFD201901&filename=1018715694.nh&uniplatform=NZKPT&v=6XgH3tAl2DWVsweqDyWcp5E6KxFMnYu1ndTASxcndGeR68_Fro6eTWHpSpA8wdh7},
	abstract = {近年来,国内农商银行面临着日益激烈的外部竞争,银行经营者越来越清晰地认识到客户对于银行的重要性。没有优良的客户资源,银行就无法发展。在这种形势下,如何利用新业务形式吸引新客户以及提高老客户的忠诚度已经成为当前国内各农商银行面临的紧迫课题。伴随着我国对外开放的经济进程,国外金融机构纷纷涌入我国,农商银行也面临着越来越激烈的竞争环境,传统的傍大户型经营模式已经很难维持银行的利润增长。另一方面,中小企业在我国支撑经济增长、维持社会稳定发展方面占据着越来越重要的作用,但中小企业的发展客观上也面临着诸多不利的因素,其中问题被认为是制约其发展的最大障碍,具体表现为可进入的渠道少、现有农商银行中小企业信贷业务体系能够对其提供的支持严重乏力。由于中小企业在资产规模、管理水平、制度保障、抗击市场环境等方面均处于劣势,与传统银行的信用评估、风险研判极难契合,导致中小企业通过传统方式向银行会遭遇审贷时间长、议价能力低、否决率高等问题,从而导致中小企业间接之路倍加困难。在这种背景下,笔者结合我国农商银行日前的贸易业务的发展趋势,对国内农商银行业务发展的必要性和意义做出合理的分析。关于如何系统的描述农商银行中小型企业创新的发展方向,这就需要我们从企业的实际需求出发结合银行满足客户的两个方面来进行分析研究。通过对农商银行与中小型企业之间实际的案例来看,我国农商银行针对中小企业信贷开发的产品对提高银行自身经营利益、降低出口企业的财务成本、缓解资金周转压力有着重大的意义。另外还对银行战略目标的发展与提高核心竞争能力产生了正面影响,农商银行可以通过这样的产品服务,实现银行与企业之间互利共赢的局面。},
	institution = {昆明理工大学},
	type = {硕士},
	author = {赵, 颢涵},
	editora = {张, 骞予},
	editoratype = {collaborator},
	urldate = {2022-08-25},
	date = {2018},
	langid = {pinyin},
	keywords = {困境, 业务, 中小企业, financing business, financing difficulties, Small and medium-sized enterprises},
}

@thesis{__2020-1,
	title = {商业银行中小企业信贷风险评估体系研究},
	url = {http://kns-cnki-net-s.vpn.uestc.edu.cn:8118/kcms/detail/detail.aspx?dbcode=CMFD&dbname=CMFD202101&filename=1020440742.nh&uniplatform=NZKPT&v=OfYxxoroy9pN0f3gityHowLmpYgRwna7eUYucH_rRs0EReOQAvsBBn5floOIFJbo},
	abstract = {随着经济快速发展、金融市场逐渐开放,国内中小企业获得了迅猛发展。近几年,中小企业的良好发展势头不仅促进了各区域经济的快速发展,同时对于我国{GDP的贡献超过了}50\%。在国家大力支持中小型企业的背景下,商业银行也顺势提供了更多的贷款业务给予中小型企业。但伴随信贷业务投入增加的同时,由此带来的信贷风险也对商业银行发展形成了影响,主要体现为不良贷款和不良贷款率都有所提升。因此,各商行要想获得贷款利润的同时必须加强对信贷风险的控制。虽然近些年商业银行加大了对信贷风险的控制,但是中小企业不良贷款依然处于较高水平,出现这种情况的缘由是商业银行中小企业信贷风险评估存在一定问题,比较突出的问题有评估体系的不完善和评估机制的不合理以及缺少现代化的评估方法等。因此本文对商业银行中小企业信贷风险评估进行研究和分析,主要通过运用logistics回归分析方法,重新构建了中小企业信贷风险评估模型。通过获取国内某股份制商业银行数据库中60家违约企业样本数据及60家信用正常企业进行数据汇总并随机按照2/3和1/3的比例分为建模和验证两个样本组。对数据进行评估指标的筛选与量化和logistics回归分析。并在以上内容基础上构建相应的模型,将数据引入模型中进行实证分析,最后将数据进行逐步回归,得出如下结论:违约指标与盈利能力、偿债能力和流动能力之间呈反向关系,与发展前景呈现正向关系。通过对商业银行中小企业信贷风险评估体系构建的结果可知,评估模型对中小型企业发生信贷违约预判的准确率达到了92.5\%,表明该风险评估体系对中小企业信贷风险评估的结果可信度较高,可以应用于同类型商行对中小企业信贷风险的评估。结合商业银行实际工作中存在的问题来看,商业银行中小企业信贷风险评估体系的完善还需要内外部、全流程上的支持。本文最后对如何更好运用信贷风险评估体系,提出了建议。},
	institution = {浙江工业大学},
	type = {硕士},
	author = {魏, 佳},
	editora = {阮, 明烽},
	editoratype = {collaborator},
	urldate = {2022-08-25},
	date = {2020},
	langid = {pinyin},
	doi = {10.27463/d.cnki.gzgyu.2020.001239},
	keywords = {评估体系, 信贷风险, 中小企业, credit risk, evaluation system, logistic regression, logistics回归, {SMEs}},
}

@thesis{4,
	title = {基于税务信息的中小企业信用评价体系研究},
	url = {http://kns-cnki-net-s.vpn.uestc.edu.cn:8118/kcms/detail/detail.aspx?dbcode=CMFD&dbname=CMFD202002&filename=1020625957.nh&uniplatform=NZKPT&v=bneTNuR0p-f2nePl-N5teHl5IpnrziB_HGComTgr7cp8v2P90Gw2aFsVoDYHNG3E},
	abstract = {信用评价方法依赖于受评对象的真实数据,而受评对象的道德风险、逆向选择,以及社会信用体系不完善等因素造成信用评价方获取真实有效数据的成本较高。信息壁垒增加了信用评价活动中评价方的信息获取成本和信息审核成本,最终提高了企业的融资成本。信息获取成本问题在中小企业信用融资活动中显得尤为突出,因为大部分中小企业的融资需求有期限短、需求急、频率高、金额小等特点,授信方（银行）在信用评价过程中耗费的时间和经济成本是部分中小企业难以承受的。为解决信用评价过程中的信息不对称问题,本文欲寻找一种低成本的信息获取途径。我国税务机关要求企业报送登记信息、纳税信息和财务信息,储备有大量有评价价值的信息。受到“银税互动”成功经验的启发,本文从信息获取的角度入手,结合现有企业信用评价的研究成果,分析如何利用税务信息更好的进行信用评价。在分析的基础上,本文设计了利用税务信息的企业信用评价方法,以期促进中小企业信用融资活动的发展。由于中小企业的复杂性,尚未有针对中小企业信用评价的统一观点。因此,本文花费较多篇幅研究了企业信用评价方法、评价模型和评价指标,然后寻找在不同信用评价过程中,税务信息能够发挥作用的切入点。},
	institution = {河南财经政法大学},
	type = {硕士},
	author = {楚, 泽昊},
	editora = {李, 冬霞},
	editoratype = {collaborator},
	urldate = {2022-08-26},
	date = {2020},
	langid = {pinyin},
	doi = {10.27113/d.cnki.ghncc.2020.000188},
	keywords = {税务信息, 信息不对称, 信用评价指标, 银税互动, 中小企业信用评价, Credit Rating, Information Asymmetry, Interaction of Bank and Tax Authority, {SMES} Credit Evaluation, Tax information},
}

